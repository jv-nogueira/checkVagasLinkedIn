{
    "manifest_version": 3,
    "name": "Coincide a descrição LinkedIn",
    "description": "Ao conter a palavra procurada na descrição, então vai ser aberta em nova página",
    "version": "0.1",
    "permissions": ["activeTab"],
    "icons": { 
        "48": "icons/automacao.png"
},

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "content.js"
            ]
        }
    ]
}